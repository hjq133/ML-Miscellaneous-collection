   "source": [
    "import cv2 as cv\n",
    "import numpy as np\n",
    "\n",
    "src = cv.imread(\"./2.jpg\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "src=cv.resize(src,None,fx=0.55,fy=0.55,interpolation=cv.INTER_AREA) # 插值缩放\n",
    "dst = cv.pyrMeanShiftFiltering(src, 25, 40, None, 2)\n",
    "cv.imshow(\"result\", np.hstack((src,dst)))\n",
    "cv.waitKey(0)\n",
    "cv.destroyAllWindows()"
   ]
  },
